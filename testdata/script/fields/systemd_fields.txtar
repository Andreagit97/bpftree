# fields full name
exec bpftree i t 1 -f 'tid,pid,ptid,ppid,rptid,rppid,comm,reaper,ns_level,vtid,vpid,pgid,vpgid,sid,vsid,exepath'
cmp stdout fields/names.txt

# fields short name
exec bpftree i t 1 -f 't,p,pt,pp,rpt,rpp,c,r,ns,vt,vp,pg,vpg,s,vs,e'
cmp stdout fields/names.txt

# Use unexistent format 'tid,pid,reaper, rpt,c'
! exec bpftree i t 1 -f 'tid,pid,reaper, rpt,c'
stdout '🔴 field '' rpt'' is not allowed'

# Use unexistent format 'tid,pid,reaper, rpt,c'
! exec bpftree i t 1 -f 'tid,pid,NOOO,rpt,c'
stdout '🔴 field ''NOOO'' is not allowed'

